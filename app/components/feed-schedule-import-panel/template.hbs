{{#collapsable-panel title="Agendamento de Importação de Feed" panelClass=isSuccessful}}
  <dl class="dl-horizontal">
    <dt>Criada a</dt>
    <dd>{{feedScheduleImport.created_at}}</dd>
    <dt>Atualizada a</dt>
    <dd>{{feedScheduleImport.updated_at}}</dd>
  </dl>
  {{#if feedScheduleImport.import_log}}
    {{#collapsable-panel title="Registo de Importação"}}
      <pre>{{feedScheduleImport.import_log}}</pre>
    {{/collapsable-panel}}
  {{/if}}
  {{#if feedScheduleImport.exception_log}}
    {{#collapsable-panel title="Registo de Problema" panelClass="panel-danger"}}
      <pre>{{feedScheduleImport.exception_log}}</pre>
    {{/collapsable-panel}}
  {{/if}}
{{/collapsable-panel}}
